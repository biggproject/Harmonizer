<xs:simpleType name="ServicioBalanceType">
  <xs:restriction base="xs:string">
    <xs:enumeration value="ACS"/>
    <xs:enumeration value="CAL"/>
    <xs:enumeration value="REF"/>
    <xs:enumeration value="VEN"/>
    <xs:enumeration value="ILU"/>
    <xs:enumeration value="HU"/>
    <xs:enumeration value="DHU"/>
    <xs:enumeration value="BAC"/>
    <xs:enumeration value="NDEF"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="ValoresBalanceType">
  <xs:restriction base="xs:string">
    <xs:pattern value="\s*[+-]?\d+(\.\d{1,2})?(\s*,\s*([+-]?\d+(\.\d{1,2})?)){11}\s*"/>
  </xs:restriction>
</xs:simpleType>

<xs:complexType name="MetadatosType">
  <xs:sequence>
    <xs:element name="Metadato" minOccurs="0" maxOccurs="unbounded">
      <xs:complexType>
        <xs:all>
          <xs:element name="Clave" type="xs:string"/>
          <xs:element name="Valor" type="xs:string"/>
        </xs:all>
      </xs:complexType>
    </xs:element> <!-- Fin de Metadato -->
  </xs:sequence>
</xs:complexType>

<xs:complexType name="FactoresDatosBalanceType">
  <xs:sequence>
    <xs:element name="Dato" maxOccurs="unbounded">
      <xs:complexType>
        <xs:sequence>
          <xs:element name="Vector" type="stringtypeVectorEnergeticoCTE"/>
          <xs:element name="Origen">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="INSITU"/>
                <xs:enumeration value="RED"/>
                <xs:enumeration value="COGENERACION"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="Destino">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="SUMINISTRO"/>
                <xs:enumeration value="A_NEPB"/>
                <xs:enumeration value="A_RED"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="Paso">
            <xs:simpleType>
              <xs:restriction base="xs:string">
                <xs:enumeration value="A"/>
                <xs:enumeration value="B"/>
              </xs:restriction>
            </xs:simpleType>
          </xs:element>
          <xs:element name="ren" type="dectype3digits"/>
          <xs:element name="nren" type="dectype3digits"/>
          <xs:element name="co2" type="dectype3digits"/>
          <xs:element name="Comentario" type="xs:string"/>
        </xs:sequence>
      </xs:complexType>
    </xs:element>
    <!-- Fin Dato -->
  </xs:sequence>
</xs:complexType>

<xs:complexType name="ComponentesDatosBalanceType">
 <xs:sequence>
    <xs:element name="Dato" maxOccurs="unbounded">
      <xs:complexType>
  <xs:sequence>
    <xs:element name="Vector" type="stringtypeVectorEnergeticoCTE"/>
    <xs:element name="Tipo">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="CONSUMO"/>
          <xs:enumeration value="PRODUCCION"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="Subtipo">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:enumeration value="EPB"/>
          <xs:enumeration value="NEPB"/>
          <xs:enumeration value="COGENERACION"/>
          <xs:enumeration value="INSITU"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:element>
    <xs:element name="Servicio" type="ServicioBalanceType"/>
    <xs:element name="Valores" type="ValoresBalanceType"/>
    <xs:element name="Comentario" type="xs:string"/>
  </xs:sequence>
      </xs:complexType>
    </xs:element>
    <!-- Fin Dato -->
  </xs:sequence>
</xs:complexType>


