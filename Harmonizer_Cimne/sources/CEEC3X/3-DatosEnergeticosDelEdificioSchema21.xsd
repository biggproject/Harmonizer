            <xs:element name="LimiteCalefaccionVivienda" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="LimiteRefrigeracionVivienda" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="LimiteAhorroOtrosUsos" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
