            <xs:element name="Conjunta" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Calefaccion08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Refrigeracion08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Conjunta08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Ahorro08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
