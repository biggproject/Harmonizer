        <xs:complexType>
        <xs:all>
            <xs:element name="Global" type="dectype2digits"/>
            <xs:element name="Calefaccion" type="dectype2digits" minOccurs="0"/>
            <xs:element name="Refrigeracion" type="dectype2digits" minOccurs="0"/>
            <xs:element name="ACS" type="dectype2digits" minOccurs="0"/>
            <!-- Elementos obsoletos desde v3.0 -->
            <xs:element name="Conjunta" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Calefaccion08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Refrigeracion08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
            <xs:element name="Conjunta08" type="dectype2digits" minOccurs="0"/><!-- Obsoleto -->
        </xs:all>
        </xs:complexType>
